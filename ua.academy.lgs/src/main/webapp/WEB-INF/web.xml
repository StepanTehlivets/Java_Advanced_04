  </servlet>
  <servlet-mapping>
    <servlet-name>ShowJournalsServlet</servlet-name>
    <url-pattern>/ShowJournalsServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddToCartServlet</servlet-name>
